{
  "expo": {
    "name": "InvestmentCoachMobileApp_NEW",
    "slug": "InvestmentCoachMobileApp_NEW",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
